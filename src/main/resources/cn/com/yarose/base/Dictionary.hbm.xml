<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.com.yarose.base" auto-import="false">
	<class name="Dictionary" table="t_dictionary" lazy="true">
		<id name="id" column="id" type="long">
			<generator class="native">
				<param name="sequence">s_dictionary_id</param>
			</generator>
		</id>
		<property name="type" type="integer" not-null="true"></property>
		<property name="name" type="string" length="200" not-null="true"></property>
	</class>
	<query name="Dictionary.findByName">from cn.com.yarose.base.Dictionary as dic where dic.type=? and dic.name=?</query>
</hibernate-mapping>